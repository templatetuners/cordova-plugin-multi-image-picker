<?xml version="1.0" encoding="UTF-8"?>
<plugin id="cordova-plugin-multi-image-picker" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <name>MultiImagePicker</name>

    <js-module src="www/MultiImagePicker.js" name="MultiImagePicker">
        <clobbers target="MultiImagePicker" />
    </js-module>

    <platform name="android">

        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="MultiImagePicker">
                <param name="android-package" value="com.justapplications.multiimagepicker.MultiImagePicker" />
                <param name="onload" value="true" />
            </feature>
        </config-file>

        <source-file src="src/android/MultiImagePicker.java" target-dir="src/com/justapplications/multiimagepicker" />
    </platform>
</plugin>
